------------------------------------------------------------------
function counter(){
  let count = 0 ;

  return function () {
    return count++;

  }
}

let count = counter()
console.log(count);
console.log(count()); // 0
console.log(count()); // 1
console.log(count()); // 2

console.log(counter()()); // 0
console.log(counter()()); // 0

