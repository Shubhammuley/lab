let visitsCountMap = new Map();

function countUser(user) {
  let count = visitsCountMap.get(user) || 0;
  visitsCountMap.set(user, count + 1);
}

let john = { name: "John" };
let a = { name: "John" };
countUser(john);
countUser(john);
countUser(a);
countUser(a);
countUser(john);
console.log(visitsCountMap);
john = null;
console.log(visitsCountMap);

