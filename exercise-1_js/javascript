function compare(a , b){
  if ( a > b)return 1;
  if (a == b)return 0;
  if (a < b)return -1;

}

let arr=[1,11,5,2,3,12,'A','z',"a","Z",'d']

console.log(arr.sort());
console.log(arr);

[1, -2, 15, 2, 0, 8].sort(function(a, b) {
  console.log(( a + " <> " + b ));
});
