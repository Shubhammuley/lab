-----------------------password validater-------------------------

/*function  isValidPassword( username , password ) {

  if( password.length < 8){
    return false;
  }
  if (password.indexOf(' ') != -1 ) {
    return false;
  }

  if ( password.indexOf(username) != -1){
    return false;
  }
  return true;
}

*/

function  isValidPassword( username , password ) {

  if ( password.length < 8 ||
       password.indexOf(' ') != -1 ||
       password.indexOf(username) != -1) {
         return false;
  }
  return true;
}
