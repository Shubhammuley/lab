  [1, 2, 3],
  [4, 5, 6],
  [7, 8, 9]
];

for ( let i = 0 ; i < 3 ; i++ ){
  for (let j = 0 ; j < 3 ; j++){
    console.log(matrix[i][j]);
    document.write( `\n ${matrix[i][j]}`)
  }
  document.write("<br>  ")
}
------------------------------------------------------------------------------------------------------------------------------------
function sumInput () {
  let n = [];

  while (true) {
    let val = prompt("enter number")

    if( val === "" || val === null || !isFinite(val)) break;

    n.push(+val)
    console.log(+val);
  }
  let sum=0;

  for ( let i in n ) {
    sum += i ;

  }
  return sum;
}



console.log(sumInput());
