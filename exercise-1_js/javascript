----------------------------------------------------------------------------------------------------Caching-------------------------
function slow(x) {
   console.log("Called " + x);
   return x;

}

function decorater(func){
  let cache = new Map();

  return function (x) {
    if(cache.has(x)){
      console.log( 'again ' + cache.get(x));
      return cache.get(x);
    }
    let result = func(x);

    cache.set(x,result)

    return result;
  }
}

 slow1 = decorater(slow);
 slow1(1)
 slow1(1)
 slow1(2)
 slow1(3)
 slow1(2)
 slow1(3)
